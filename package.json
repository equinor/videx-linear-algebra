{
  "name": "@equinor/videx-linear-algebra",
  "version": "1.0.12",
  "description": "A library with linear algebra used by Videx.",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.umd.js",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "prepub": "npm run build",
    "pub": "npm publish --access=public",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "predocs": "rimraf docs",
    "docs": "typedoc --out docs src",
    "postdocs": "copyfiles images/* docs && copyfiles .nojekyll docs",
    "lint": "eslint --color"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/equinor/videx-linear-algebra.git"
  },
  "keywords": [
    "videx",
    "linear",
    "algebra"
  ],
  "contributors": [
    "Kjerand Pedersen",
    "Tom Kristian Tjemsland"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/equinor/videx-linear-algebra/issues"
  },
  "homepage": "https://github.com/equinor/videx-linear-algebra#readme",
  "devDependencies": {
    "@equinor/videx-vector2": "^1.0.47",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "copyfiles": "^2.2.0",
    "eslint": "^9.32.0",
    "eslint-plugin-import": "^2.31.0",
    "jest": "^29.7.0",
    "rimraf": "^6.1.0",
    "rollup": "^4.52.5",
    "ts-jest": "^29.4.5",
    "tslib": "^2.8.1",
    "typedoc": "^0.28.14",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@equinor/videx-math": "^1.1.3"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "/test/.*",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
