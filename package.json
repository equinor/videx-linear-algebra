{
  "name": "@equinor/videx-linear-algebra",
  "version": "1.0.4",
  "description": "A library with linear algebra used by Videx.",
  "main": "dist/bundle.cjs.js",
  "module": "dist/bundle.esm.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "prepub": "npm run build",
    "pub": "npm publish --access=public",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "predocs": "rimraf docs",
    "docs": "node_modules/.bin/jsdoc -c jsdoc.json",
    "postdocs": "copyfiles images/* docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/equinor/videx-linear-algebra.git"
  },
  "keywords": [
    "videx",
    "linear",
    "algebra"
  ],
  "contributors": [
    "Kjerand Pedersen",
    "Tom Kristian Tjemsland"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/equinor/videx-linear-algebra/issues"
  },
  "homepage": "https://github.com/equinor/videx-linear-algebra#readme",
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-transform-modules-commonjs": "^7.6.0",
    "@babel/preset-env": "^7.6.3",
    "copyfiles": "^2.1.1",
    "docdash": "^1.1.1",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "jest": "^24.9.0",
    "jsdoc": "^3.6.3",
    "rimraf": "^3.0.0",
    "rollup": "^1.26.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.1.2"
  },
  "dependencies": {
    "@equinor/videx-math": "^1.0.10"
  }
}
